{
  "name": "solid-boring-avatars",
  "version": "0.0.0",
  "description": "Boring avatars is a tiny JavaScript SolidJS library that generates custom, SVG-based avatars from any username and color palette.",
  "license": "MIT",
  "author": "blopker",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totem-technologies/solid-boring-avatars.git"
  },
  "homepage": "https://github.com/totem-technologies/solid-boring-avatars#readme",
  "bugs": {
    "url": "https://github.com/totem-technologies/solid-boring-avatars/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently npm:test:*",
    "test:client": "vitest",
    "test:ssr": "npm run test:client --mode ssr",
    "prepublishOnly": "npm run build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently npm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "npm update --save"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.15",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.0",
    "solid-devtools": "^0.30.1",
    "solid-js": "^1.7.8",
    "tsup": "^8.0.0",
    "tsup-preset-solid": "^2.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.33.0"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  },
  "dependencies": {
    "@tiagotrindade/solid-colorful": "^1.0.2",
    "nice-color-palettes": "^3.0.0",
    "solid-styled-components": "^0.28.5"
  }
}
